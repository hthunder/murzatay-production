{
    "name": "blog",
    "version": "1.0.0",
    "description": "it's a blog about cats",
    "main": "server.js",
    "private": true,
    "engines": {
        "npm": "^6.14.16"
    },
    "scripts": {
        "start": "node server",
        "start:dev": "npm-run-all --parallel server scss webpack",
        "install:production": "npm install --only=prod",
        "server": "nodemon server --ignore public/img/instagram/compressed/img-list.json",
        "mongo": "mkdir -p /workspace/data && mongod --dbpath /workspace/data",
        "mongo:mac": "brew services start mongodb-community@5.0",
        "mongo:debian": "systemctl start mongod",
        "scss": "npx sass --watch public/scss:public/css",
        "scss:production": "npx sass public/scss:public/css --style=compressed",
        "webpack": "npx webpack --mode=development",
        "webpack:production": "webpack --mode=production",
        "eslint": "eslint",
        "eslint:fix": "eslint --fix",
        "build": "npm run scss:production && npm run webpack:production",
        "deploy": "npm run build && rsync --archive --compress --exclude='.*' --exclude='public/scss' --exclude='node_modules' --exclude='README.md' --exclude='webpack.config.js' . root@2.57.186.199:~/murzatay"
    },
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/hthunder/Murzatay/issues"
    },
    "homepage": "https://github.com/hthunder/Murzatay#readme",
    "dependencies": {
        "awesome-notifications": "^3.1.2",
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "config": "^3.3.3",
        "cookie-parser": "^1.4.5",
        "dompurify": "^2.2.6",
        "dotenv": "^8.2.0",
        "escape-html": "^1.0.3",
        "express": "^4.17.1",
        "express-handlebars": "^5.2.0",
        "express-validator": "^6.8.0",
        "gravatar": "^1.8.1",
        "hbs": "^4.1.2",
        "imagemin": "^7.0.1",
        "imagemin-mozjpeg": "^9.0.0",
        "jsdom": "^16.4.0",
        "jsonwebtoken": "^8.5.1",
        "lodash.escape": "^4.0.1",
        "method-override": "^3.0.0",
        "mongoose": "^5.11.15",
        "multer": "^1.4.2",
        "nodemailer": "^6.4.17",
        "pug": "^3.0.0",
        "request": "^2.88.2",
        "slugify": "^1.4.6",
        "tinymce": "^5.7.1"
    },
    "devDependencies": {
        "@babel/core": "^7.14.6",
        "@babel/plugin-proposal-optional-chaining": "^7.14.5",
        "@babel/preset-env": "^7.14.7",
        "babel-loader": "^8.2.2",
        "concurrently": "^5.3.0",
        "css-loader": "^6.4.0",
        "csscomb": "^4.3.0",
        "eslint": "^7.22.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.3.1",
        "nodemon": "^2.0.6",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.3.3",
        "postcss-cli": "^8.3.1",
        "postcss-scss": "^4.0.0",
        "postcss-sorting": "^6.0.0",
        "prettier": "^2.2.1",
        "sass": "^1.32.13",
        "style-loader": "^3.3.0",
        "stylelint": "^13.13.1",
        "stylelint-config-idiomatic-order": "^8.1.0",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-sass-guidelines": "^8.0.0",
        "stylelint-config-standard": "^22.0.0",
        "stylelint-order": "^4.1.0",
        "webpack": "^5.36.2",
        "webpack-cli": "^4.6.0"
    }
}
